<%= form_for [@course, @review] do |f| %>
  <div id="review_rating"></div>
  
  <%= f.text_field :comment, placeholder: "Leave your comments here"  %>

  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :course_id %>

  <div class="actions">
    <%= f.submit "Submit Review" %>
  </div>
<% end %>
